{
  "dns": {
    "disableFallbackIfMatch": true,
    "hosts": {
      "domain:googleapis.cn": "googleapis.com"
    },
    "servers": [
      {
        "address": "https://1.1.1.1/dns-query"
      },
      {
        "address": "https+local://223.5.5.5/dns-query",
        "domains": [
          "yourdomain"
        ],
        "skipFallback": true
      }
    ]
  },
  "inbounds": [
    {
      "listen": "127.0.0.1",
      "port": 2080,
      "protocol": "http",
      "settings": {
        "auth": "noauth",
        "udp": true
      }
    }
  ],
  "log": {
    "loglevel": "debug"
  },
  "outbounds": [
    {
      "domainStrategy": "AsIs",
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "yourdomain",
            "port": 12345,
            "users": [
              {
                "alterId": 0,
                "id": "uuid",
                "security": "auto"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "headers": {},
          "path": "websocketpath"
        }
      }
    },
    {
      "protocol": "freedom",
      "tag": "direct"
    },
    {
      "protocol": "freedom",
      "tag": "bypass"
    },
    {
      "protocol": "dns",
      "settings": {
        "address": "1.1.1.1",
        "network": "tcp",
        "userLevel": 1
      },
      "tag": "dns-out"
    }
  ],
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "inboundTag": [
          "dns-in"
        ],
        "outboundTag": "dns-out",
        "type": "field"
      },
      {
        "outboundTag": "dns-out",
        "protocol": [
          "dns"
        ],
        "type": "field"
      }
    ]
  }
}
